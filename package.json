{
  "name": "electron-webpack",
  "version": "1.0.0",
  "license": "MIT",
  "main" : "",
  "scripts": {
    "electron:watch": "NODE_ENV=desktop webpack --watch",
    "electron:engine": "electron ./dist/Desktop/main.js",
    "desktop:prod": "rimraf dist/Desktop && NODE_ENV=desktop:prod webpack",
    "desktop:dev": "npm-run-all -p electron:engine electron:watch",
    "desktop:make": "cd dist/Desktop && electron-packager . DesktopApp --overwrite",
    "cordova:watch": "NODE_ENV=mobile webpack --watch",
    "cordova:build": "NODE_ENV=mobile:prod webpack",
    "cordova:engine": "cd dist/Mobile &&  taco run android --livereload",
    "cordova:prepare": "rimraf dist/Mobile && cd dist/ && taco create Mobile && cd Mobile/ && taco platform add browser android && rm www/* -rf",
    "cordova:browser": "cd dist/Mobile &&  taco run browser --port=8000 --target=Firefox",
    "mobile:prod": "npm-run-all -s cordova:prepare cordova:build",
    "mobile:dev": "npm-run-all -p cordova:watch cordova:engine",
    "mobile:browser": "npm-run-all -p  cordova:watch cordova:browser"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-babili": "0.0.12",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-0": "^6.22.0",
    "babili-webpack-plugin": "0.0.11",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "electron": "^1.6.2",
    "electron-packager": "^8.6.0",
    "electron-reload": "^1.1.0",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "eslint-config-vue": "^2.0.2",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^2.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.0",
    "npm-run-all": "^4.0.2",
    "postcss": "^5.2.16",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^1.3.3",
    "postcss-reporter": "^3.0.0",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.3",
    "sasslint-webpack-plugin": "^1.0.4",
    "style-loader": "^0.16.1",
    "taco-cli": "^1.2.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
